{
  "root": true,
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "plugins": ["prettier", "jest"],
  "extends": ["eslint:recommended", "plugin:jest/recommended"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
      }
    ],
    "no-unused-vars": "off"
  }
}
